<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Propietats per connectar amb servidor JRS</comment>
    <entry key="url">http://localhost:8080/jasperserver/rest_v2/reports/Reports/</entry>
    <!--En aquesta url es suposa que JRS està escoltant pel port 8080 
        i a l'arrel de l'usuari té una carpeta "informes" amb subcarpeta "projecte1"-->
    <entry key="user">jasperadmin</entry>
    <entry key="password">jasperadmin</entry>
</properties>
